[
  {
    "ClientId": "IgrManagerMvc",
    "ClientName": "IGR Manager MVC Client",
    "RequireConsent": false,
    "RequirePkce": true,
    "RedirectUris": [
      "http://localhost:6002/signin-oidc"
    ],
    "properties": {
      "version": "10"
    },
    "PostLogoutRedirectUris": [
      "http://localhost:6002/signout-callback-oidc"
    ],
    "AllowOfflineAccess": true,
    "AllowedScopes": [
      "openid",
      "profile",
      "email",
      "address",
      "phone",
      "IGRMgr.API"
    ],
    "AccessTokenLifetime": 1800,
    "IdentityTokenLifetime": 1800
  }
]